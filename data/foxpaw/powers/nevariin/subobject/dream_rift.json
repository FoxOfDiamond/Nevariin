{
    "hidden": true,
    "type": "origins:multiple",
    "callback":{
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "/function foxpaw:nevariin/global/forceloader/forceload_self"
        }
    },
    "cooldown": {
        "type": "origins:resource",
        "min": 0,
        "max": 60,
        "start_value": 0
    },
    "lifetime": {
        "type": "origins:resource",
        "min": 0,
        "max": 6000,
        "start_value": 6000
    },
    "tick": {
        "type": "origins:action_over_time",
        "interval": 5,
        "entity_action": {
            "type": "origins:and",
            "actions": [{
                    "type": "origins:change_resource",
                    "change": 2,
                    "operation": "add",
                    "resource": "*:*_cooldown"
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "*:*_lifetime",
                        "compare_to": 0,
                        "comparison": "=="
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "/function foxpaw:nevariin/dream_rift/clear"
                    },
                    "else_action": {
                        "type": "origins:change_resource",
                        "change": -5,
                        "operation": "add",
                        "resource": "*:*_lifetime"
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "/function foxpaw:nevariin/dream_rift/ambient_particle"
                },
                {
                    "type": "origins:if_else_list",
                    "actions": [{
                            "condition": {
                                "type": "origins:resource",
                                "compare_to": 10,
                                "comparison": "<",
                                "resource": "*:*_cooldown"
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "/function foxpaw:nevariin/dream_rift/cooldown_particle_0"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "compare_to": 20,
                                "comparison": "<",
                                "resource": "*:*_cooldown"
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "/function foxpaw:nevariin/dream_rift/cooldown_particle_1"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "compare_to": 30,
                                "comparison": "<",
                                "resource": "*:*_cooldown"
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "/function foxpaw:nevariin/dream_rift/cooldown_particle_2"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "compare_to": 40,
                                "comparison": "<",
                                "resource": "*:*_cooldown"
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "/function foxpaw:nevariin/dream_rift/cooldown_particle_3"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "compare_to": 50,
                                "comparison": "<",
                                "resource": "*:*_cooldown"
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "/function foxpaw:nevariin/dream_rift/cooldown_particle_4"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "compare_to": 60,
                                "comparison": "<",
                                "resource": "*:*_cooldown"
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "/function foxpaw:nevariin/dream_rift/cooldown_particle_5"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "compare_to": 60,
                                "comparison": "==",
                                "resource": "*:*_cooldown"
                            },
                            "action": {
                                "type": "origins:and",
                                "actions": [{
                                        "type": "origins:execute_command",
                                        "command": "/function foxpaw:nevariin/dream_rift/ready_particle"
                                    },
                                    {
                                        "type": "origins:area_of_effect",
                                        "radius": 0.75,
                                        "shape": "cube",
                                        "bientity_action": {
                                            "type": "origins:and",
                                            "actions": [{
                                                    "type": "origins:target_action",
                                                    "action": {
                                                        "type": "origins:execute_command",
                                                        "command": "tag @s add nevariin_rift_target"
                                                    }
                                                },
                                                {
                                                    "type": "origins:actor_action",
                                                    "action": {
                                                        "type": "origins:execute_command",
                                                        "command": "/function foxpaw:nevariin/dream_rift/forceload"
                                                    }
                                                },
                                                {
                                                    "type": "origins:actor_action",
                                                    "action": {
                                                        "type": "origins:execute_command",
                                                        "command": "/execute as @e[tag=nevariin_dream_rift] run resource set @s foxpaw:nevariin/subobject/dream_rift_cooldown 0"
                                                    }
                                                },
                                                {
                                                    "type": "origins:actor_action",
                                                    "action": {
                                                        "type": "origins:execute_command",
                                                        "command": "/execute at @s as @e[tag=nevariin_rift_target] run function foxpaw:nevariin/dream_rift/shift"
                                                    }
                                                },
                                                {
                                                    "type": "origins:target_action",
                                                    "action": {
                                                        "type": "origins:execute_command",
                                                        "command": "tag @s remove nevariin_rift_target"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    }
}